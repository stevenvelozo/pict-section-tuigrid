{
	"name": "invoice_grid",
	"version": "1.0.0",
	"description": "Invoice grid example with custom data marshaling for pict-section-tuigrid",
	"main": "Invoice-Grid-Application.js",
	"scripts":
	{
		"start": "node Invoice-Grid-Application.js",
		"build": "npx quack build && npx quack copy"
	},
	"author": "steven velozo <steven@velozo.com>",
	"license": "MIT",
	"copyFilesSettings":
	{
		"whenFileExists": "overwrite"
	},
	"copyFiles":
	[
		{ "from": "./html/*", "to": "./dist/" },
		{ "from": "./node_modules/tui-grid/dist/*", "to": "./dist/" },
		{ "from": "../../node_modules/pict/dist/*", "to": "./dist/" }
	],
	"dependencies":
	{
		"pict": "^1.0.293",
		"pict-application": "^1.0.19",
		"pict-view": "^1.0.63",
		"tui-grid": "^4.21.22"
	}
}
